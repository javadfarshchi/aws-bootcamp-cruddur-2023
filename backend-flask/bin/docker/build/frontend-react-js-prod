--build-arg REACT_APP_AWS_USER_POOLS_ID="ca-central-1_JF7hsodKV" \
--build-arg REACT_APP_CLIENT_ID="4qi33takg94f3oeq5db5p71qdi" \
-t frontend-react-js \
-f Dockerfile.prod \
.